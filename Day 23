#include <iostream>
using namespace std;

int main() 
{
    int t,a,b,c;
    cin>>t;
    while (t--)
    {
        cin>>a>>b>>c;
        if(a>b)
        {
            if(b>c)
            {
                cout<<b<<"\n";
            }
            else if(c<a)
            
            {
                cout<<c<<"\n";
            }
            else
            {
                cout<<a<<"\n";
            }
               
        }
        else
        {
            if(a>c)
            {
                cout<<a<<"\n";
            }
            else if (c<b)
            {
                cout<<c<<"\n";
            }  
            else
            {
                cout<<b<<"\n";
            }
         }
            
    }
  
  
	return 0;
}
