//code1
//{ Driver Code Starts
#include <bits/stdc++.h>

using namespace std;

// } Driver Code Ends
class Solution {
  public:
    void printTriangle(int n) {
        //{ Driver Code Starts
        for(int j=0;j<n;j++)
        {
            for(int i=0;i<=j;i++)
            {
            cout<<i+1<<" ";
            }
            cout<<"\n";
        }
    };
};

//{ Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;

        Solution ob;
        ob.printTriangle(n);
    }
    return 0;
}
// } Driver Code Ends

