#include <iostream>
using namespace std;

int main() {
	int t,x;
	cin>>t;
	 for(int i=0;i<t;i++)
	 {
	     cin>>x;
	     if((x%10)==0)
	     {
	         cout<<x/10<<"\n";
	     }
	     else if((x%5)==0)
	     {
	         cout<<(x/10)+((x%10)/5)<<"\n";
	     }
	     else
	     {
	         cout<<"-1"<<"\n";
	     }
	 }
	return 0;
}
